        stylesheets: [
            '/totemcss/templates/default/stylesheets/default.css',
            '/totemcss/groups/frontpage/stylesheets/frontpage.css',
            '/totemcss/modules/totemcss-module-tipi/stylesheets/totemcss-module-tipi.css',
